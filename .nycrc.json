{
  "include": [
    "modules/**/*.js",
    "app.js"
  ],
  "exclude": [
    "test/**",
    "examples/**",
    "node_modules/**",
    "**/*.test.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "report-dir": "coverage",
  "temp-dir": ".nyc_output",
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 70,
  "cache": true,
  "all": true
}
